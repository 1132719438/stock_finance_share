    <tr data-toggle="modal" data-target="#momal-accounts-receivable-turnover-ratio">
      <td class="theader-style" rowspan="6">经营能力</td>
      <td align=left>应收账款周转率(次)</td>
      <% (0..4).reverse_each.each do |i| %>
        <td align=center width = "15.4%">
          <%= @stock_a_static_data[9][i] %>
          / <%= @stock_b_static_data[9][i] %>
          <% if !@stock_c.blank? %>
          / <%= @stock_c_static_data[9][i] %>
          <% end %>
        </td>
      <% end %>
    </tr>

    <tr data-toggle="modal" data-target="#momal-accounts-receivable-turnover-ratio-360">
      <td align=left>平均收现日数</td>
      <% (0..4).reverse_each.each do |i| %>
        <td align=center width = "15.4%">
          <% if @stock_a_static_data[9][i] == 0 %>
            NA
          <% else %>
            <%= (360 / @stock_a_static_data[9][i]).round(1) %>
          <% end %>
          <% if @stock_b_static_data[9][i] == 0 %>
            NA
          <% else %>
            / <%= (360 / @stock_b_static_data[9][i]).round(1) %>
          <% end %>
          <% if !@stock_c.blank? %>
            <% if @stock_c_static_data[9][i] == 0 %>
              NA
            <% else %>
