<%= simple_form_for [:admin, @us_stock] do |f| %>

  <div class="form-group">
    <%= f.input :symbol, :label => "代码/symbol", input_html: { class: "form-control"} %>
    <%= f.input :easy_symbol, :label => "代码/easy_symbol", input_html: { class: "form-control"} %>
    <%= f.input :name, :label => "名称/cnname", input_html: { class: "form-control"} %>
    <%= f.input :cnname, :label => "中文名称/cnname", input_html: { class: "form-control"} %>
    <%= f.input :ipoyear, :label => "上市时间/ipoyear", input_html: { class: "form-control"} %>
    <%= f.input :pyramid_rating, :label => "金字塔积分/pyramid_rating", input_html: { class: "form-control"} %>
    <hr>
    <%= f.input :sector, :label => "领域/sector", input_html: { class: "form-control"} %>
    <%= f.input :industry, :label => "行业/industry", input_html: { class: "form-control"} %>
    <!-- <%= f.input :main_business, :label => "主营业务/main_business", input_html: { class: "form-control"} %> -->
    <hr>
    <!-- <%= f.input :cwzb, :label => "财务指标/cwzb", input_html: { class: "form-control"} %> -->
    <%= f.input :lrb, :label => "利润表/lrb", input_html: { class: "form-control"} %>
    <%= f.input :llb, :label => "流量表/lrb", input_html: { class: "form-control"} %>
    <%= f.input :zcb, :label => "资产负债表/zcb", input_html: { class: "form-control"} %>
    <hr>
    <%= f.input :static_data, :label => "静态数据/static_data", input_html: { class: "form-control"} %>
    <hr>
    <%= f.submit "提交", :class => "btn btn-sm btn-default", :data => { disable_with: "Submiting..." } %>
    <%= link_to("删除", admin_us_stock_path(@us_stock), method: :delete, :class => "btn btn-sm btn-danger", data: { confirm: "确定删除" }) %>

  </div>

<% end %>
